#include <iostream>
#include <vector>
using namespace std;

class Solution {
public:
    // Function to perform linear search in a vector
    int linearSearch(vector<int>& nums, int target) {
        int n = nums.size(); // Get the size of the vector

        // Traverse through each element in the vector
        for (int i = 0; i < n; i++) {
            // If the current element matches the target, return its index
            if (nums[i] == target) {
                return i; // Element found at index i
            }
        }
        
        // If the element was not found, return -1
        return -1;
    }
};
